<div class="modal">

  <div class="modal-overlay modal-toggle"></div>

  <div class="modal-wrapper modal-transition">

    <div class="modal-header">

      <button class="modal-close modal-toggle">Close</button>

      <h2 class="modal-heading">Unpublish Mugshot</h2>

    </div>

    <div class="modal-body">

      <div class="modal-content">

        <center>

          <%= @mugshot.name.upcase %>

          <div class="mugshot-photos-photo"><%= image_tag  @mugshot.photos.last.image.url(:medium) %></div>

          <%#= link_to 'Unpublish',  {:controller => "removals", :action => "create", :mugshotID => @mugshot.id, :name => @mugshot.name, :email => 'blakegeist@gmail.com', :county => @mugshot.county, :charges => @mugshot.charges, :photo => @mugshot.photos.last}, :method => :post %>

        </center>

      </div>

    </div>

  </div>
