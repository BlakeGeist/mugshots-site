<!DOCTYPE html>
<html>
  <head>
    <title>MugshotsSite</title>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <nav>
        <%= link_to 'Home', root_path %> <%= link_to "Sign out", destroy_user_session_path, :method => :delete if user_signed_in?%>
      </nav>
    </header>
    <%= yield %>
  </body>
</html>
